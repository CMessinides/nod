{
  "name": "nod",
  "version": "0.0.1",
  "description": "Notes and tasks, all in one place.",
  "main": "server.js",
  "repository": "git@github.com:CMessinides/nod.git",
  "author": "Cameron Messinides <cameron.messinides@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "concurrently --kill-others \"yarn dev:proxy\" \"yarn dev:api\"",
    "dev:api": "nodemon --watch api api-server.js",
    "dev:proxy": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "apollo-server-express": "^2.5.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.5.1",
    "graphql": "^14.3.1",
    "isomorphic-fetch": "^2.2.1",
    "next": "^8.1.0",
    "pg": "^7.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/express": "^4.16.1",
    "@types/express-http-proxy": "^1.5.9",
    "@types/next": "^8.0.5",
    "@types/pg": "^7.4.14",
    "@types/react": "^16.8.19",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react-testing-library": "^7.0.1"
  }
}
